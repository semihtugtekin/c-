    internal class Program
    {
        static void Main(string[] args)
        {
            // Ders notu =>  50 ve üzerinden ise dersten geçsin

            int not = 30;

            if ( not >= 50 ) 
            {
                Console.WriteLine("GEÇTİN");
            } else
            {
                Console.WriteLine("KALDIN");
            }

            Console.WriteLine("PROGRAM BİTTİ");
            Console.ReadKey();
        }
    }
